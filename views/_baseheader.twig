        {% set mask = diary[0] %}
        {# style="background: url('{{ site.theme.link }}/assets/images/logo.png') center/contain no-repeat;" #}
        <div class="logo hidden" data-svg="{{ site.theme.link }}/assets/images/svg/logo.svg" data-mask="{{ TimberImage(mask.get_field('cover')) }}">
            <svg viewBox="0 0 40 40">
                <use xlink:href="#logo"></use>
            </svg>
        </div>
        <div class="project-list">
            <span class="list-close link">Close</span>
            <div class="vertical-center">
                <div class="vertical-el">
                    <div class="device-content">
                        {% for post in diary %}
                            <a href="/diary/{{ post.slug }}" data-index="{{ loop.index }}" data-mask="{{ TimberImage(post.get_field('cover')) }}">{{ post.title }}</a>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
        <nav class="hidden">
            <a href="/diary">Diary</a>
            <a href="/profile">Profile</a>
            <a href="/contact">Contact</a>
        </nav>
        <a href="/diary" class="name hidden">
            <svg viewBox="0 0 277.4 53.6">
               <use xlink:href="#name"></use>
            </svg>
        </a>
        <div class="arrow hidden">
            <svg viewBox="0 0 40 40">
                <path d="M20.4,11.5v12.6c0.8-0.6,1.3-0.9,2.1-0.9c-1.2,1.4-1.9,3.2-2.5,5.3c-0.6-2.2-1.3-3.9-2.5-5.3c0.8,0.1,1.3,0.3,2.1,1V11.5H20.4z"/>
            </svg>
        </div>
        <div class="menu">
            <div class="vertical-center">
                <div class="vertical-el">
                    <a href="/diary" class="link">Diary</a>
                    <a href="/profile" class="link">Profile</a>
                    <a href="/contact" class="link">Contact</a>
                </div>
            </div>
        </div>
        <span class="mobile-menu">Menu</span>
        <a href="mailto:" target="_blank" class="email hidden">info@alisharaf.com</a>
        <div id="js-view" class="page-view">{% endif %}